{
  "name": "@deepblue/toolkit",
  "version": "2.0.8",
  "description": "toolkit scripts and utilities",
  "main": "dist/es5/index",
  "module": "src/index",
  "repository": {
    "type": "git",
    "url": "git@git.db-n.com:dbn-kickstart/toolkit.git"
  },
  "keywords": [
    "services",
    "utilities"
  ],
  "author": "Darius Sobczak<darius.sobczak@db-n.com>",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "dependencies": {
    "rxjs": "^6.3.0",
    "rxjs-compat": "^6.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.7.0",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "regenerator-runtime": "^0.11.1"
  },
  "scripts": {
    "test": "jest src --watch",
    "build": "npm run build-es5",
    "build-es5": "babel src --out-dir dist/es5 --ignore spec.js,test.js",
    "build:watch": "cd src && update-call .*js$ babel %0 --out-dir ../dist",
    "build-docs": "jsdoc -r src -d docs"
  }
}
